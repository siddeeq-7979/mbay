            <div class="container">
                <!-- PC HEADER -->
                <div class="header-large-device">
                    <div class="header-top header-top-ptb-2 border-bottom-1">
                        <div class="row align-items-center justify-content-center">
                            <div class="col-xl-2 col-lg-2">
                                <div class="logo">
                                    <a href="{{BASE_URL}}index">
                                        <img src="assets/images/logos/{{COMPANY_LOGO}}" alt="mbay logo">
                                    </a>
                                </div>
                            </div>
                            <div class="col-xl-6 col-lg-2">
                                <!-- PC VIEW SEARCH AREA -->
                                <div class="mobile-view-search d-none d-md-none d-lg-none d-xl-block">
                                    <div class="input-group">
                                        <input type="text" class="typeahead form-control shadow-none"
                                            placeholder="{{lang('what_are_you_looking_for')}}"
                                            aria-label="Recipient's username" aria-describedby="button-addon2"
                                            id="search_box">
                                        <a href="javascript:void(0)" class="btn btn-outline-secondary"
                                            type="button" id="button-addon2"> <i class="icon-magnifier"></i></a>
                                    </div>
                                </div>
                                <!-- END PC VIEW SEARCH AREA -->
                            </div>
                            <div class="col-xl-4 col-lg-5">
                                <div class="header-top-right">
                                    {% if user_name %}
                                    <div class="header-login-regi">
                                        <a href="{{BASE_URL}}login-site">
                                            <i class="icon-user"></i> {{user_name}}
                                        </a>
                                    </div>
                                    {% else %}
                                    <div class="header-login-regi">
                                        <a href="{{BASE_URL}}login-site">
                                            <i class="icon-user"></i>{{lang('login')}} / {{lang('register')}} </a>
                                    </div>
                                    {% endif %}
                                    <!--language-->
                                     {%if MLM_LANG_CODE =='en' %} 
                                            <div class="header-login-regi">
                                                <a href="javascript:;" onclick="switchLanguage('ar');" title="">عربى </a>
                                            </div> 
                                        {%elseif MLM_LANG_CODE =='ar' %} 
                                            <div class="header-login-regi">
                                                <a href="javascript:;" onclick="switchLanguage('en');" title="">English</a>
                                            </div> 
                                        {%endif%} 
                                    <!--end language-->

                                    {% if items %}
                                    <div class="header-cart-2" id="header_cart">
                                        <a class="cart-active" href="{{BASE_URL}}cart">
                                            <i class="icon-basket-loaded"></i>
                                            <span class="black">{{pro_count}}</span>
                                        </a>
                                    </div>

                                    {% else %}
                                    <div class="header-cart-2" id="header_cart">
                                        <a class="cart-active" href="{{BASE_URL}}cart"><i
                                                class="icon-basket-loaded"></i> <span
                                                class="black">{{pro_count}}</span></a>
                                    </div>
                                    {% endif %}
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="header-bottom">
                        <div class="row align-items-center">
                            <div class="col-12">
                                <div
                                    class="main-menu main-menu-padding-2 main-menu-center main-menu-font-width-400 main-menu-lh-1">
                                    <nav>
                                        <ul>
                                            {% for nav in nav_category %}
                                            <li>
                                                <a href="{{BASE_URL}}shop/{{nav.id}}">{{nav.category}} </a>
                                            </li>
                                            {% endfor %}
                                        </ul>
                                    </nav>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- END PC HEADER -->

                <!-- MOBILE HEADER -->
                <div class="header-small-device small-device-ptb-1">
                    <div class="row align-items-center">
                        <div class="col-5">
